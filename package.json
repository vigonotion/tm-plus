{
  "name": "tm-plus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"panda --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:preview": "npm run pages:build && wrangler pages dev .vercel/output/static --compatibility-date=2024-01-29 --compatibility-flag=nodejs_compat",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "prepare": "panda codegen",
    "postinstall": "npm run prepare"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "lucide-react": "^0.321.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-resizable-panels": "^2.0.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.6",
    "@cloudflare/workers-types": "^4.20240129.0",
    "@pandacss/dev": "^0.29.1",
    "@radix-ui/colors": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.8.6",
    "eslint-plugin-react-refresh": "^0.4.5",
    "pandacss-preset-radix-colors": "^0.2.0",
    "typescript": "^5",
    "vercel": "^33.4.1",
    "wrangler": "^3.26.0"
  }
}
